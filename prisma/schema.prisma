generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_CONNECTION")
}

model Test {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  Test2     Test2[]
}

model Test2 {
  id     Int    @id @default(autoincrement())
  name   String
  test   Test   @relation(fields: [testID], references: [id])
  testID Int
}
